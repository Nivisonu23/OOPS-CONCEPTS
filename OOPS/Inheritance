class Professor
{
  String designation="Professor";
  String college="SJIT";
  void does()
  {
    System.out.println("Teaching");
  }
}
public class OOPSProfessor extends Professor
{
  String mainsub="OOPS";
  public static void main(String args[])
  {
    OOPSProfessor o = new OOPSProfessor();
    System.out.println(o.college);
    System.out.println(o.designation);
    System.out.println(o.mainsub);
    o.does();
  }
}
  
